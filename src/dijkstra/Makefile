	$(CC) $(CFLAGS) $(CPPFLAGS) -c -o $@ $<

$(OBJ_DIR)/$(FOLDER)/speed.o: $(SRC_DIR)/$(FOLDER)/speed.c $(INCLUDE_DIR)/$(FOLDER)/speed.h
	$(CC) $(CFLAGS) $(CPPFLAGS) -c -o $@ $<