FOLDER = dijkstra

$(OBJ_DIR)/$(FOLDER)/dijkstra.o: $(SRC_DIR)/$(FOLDER)/dijkstra.c $(INCLUDE_DIR)/$(FOLDER)/dijkstra.h
	$(CC) $(CFLAGS) $(CPPFLAGS) -c -o $@ $<

