FOLDER = utils

$(OBJ_DIR)/$(FOLDER)/utils.o: $(SRC_DIR)/$(FOLDER)/utils.c $(INCLUDE_DIR)/$(FOLDER)/utils.h
	$(CC) $(CFLAGS) $(CPPFLAGS) -c -o $@ $< $(LDLIBS)

