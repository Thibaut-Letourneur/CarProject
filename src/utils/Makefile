	$(CC) $(CFLAGS) $(CPPFLAGS) -c -o $@ $< $(LDLIBS)

$(OBJ_DIR)/$(FOLDER)/array.o: $(SRC_DIR)/$(FOLDER)/array.c $(INCLUDE_DIR)/$(FOLDER)/array.h
	$(CC) $(CFLAGS) $(CPPFLAGS) -c -o $@ $< $(LDLIBS)

$(OBJ_DIR)/$(FOLDER)/follow_line.o: $(SRC_DIR)/$(FOLDER)/follow_line.c $(INCLUDE_DIR)/$(FOLDER)/follow_line.h
	$(CC) $(CFLAGS) $(CPPFLAGS) -c -o $@ $< $(LDLIBS)
